            </div>
        </form>
    </div>
    <script type='text/javascript' src='//s3.amazonaws.com/downloads.mailchimp.com/js/mc-validate.js'></script>
    <script type='text/javascript'>(function ($) { window.fnames = new Array(); window.ftypes = new Array(); fnames[0] = 'EMAIL'; ftypes[0] = 'email'; fnames[1] = 'NAME'; ftypes[1] = 'text'; fnames[3] = 'MSG'; ftypes[3] = 'text'; }(jQuery)); var $mcj = jQuery.noConflict(true);
      let formSent = '';
      var observer = new MutationObserver(function(mutations) {
        mutations.forEach(function(mutationRecord) {
            if (formSent == 'news') newsletterFormEndAnimation()
            if (formSent == 'popup') PopupFormEndAnimation()
        });    
      });
      var target = document.getElementById('mce-success-response');
      observer.observe(target, { attributes : true, attributeFilter : ['style'] });

      function newsletterFormEndAnimation() {
        var btnTxt = $('#news-submit').val()
        $('.newsletter-section_content_form-block').css('opacity', '0.5')
        $('#mc-news-form').find("input[type=text],input[type=email]").val("");
        $('#news-submit').val('Pronto!')
        setTimeout(function () {
          $('.newsletter-section_content_form-block').css('opacity', '1')
          $('#news-submit').val(btnTxt)
        }, 2000);
      }

      function PopupFormEndAnimation() {
        var btnTxt = $('#popup-submit').val()
        $('#popup-submit').val('Pronto!')
        $('#mc-popup-form').find("input[type=text],input[type=email], textarea").val("");
        $('.talk-block').css('opacity', '0.5')
        setTimeout(function () {
          $('.talk-block').css('opacity', '1')
          $('popup-submit').val(btnTxt)
          document.getElementById("close-modal-button").click();
        }, 2000);
      }

      function sendPopupForm(){
        formSent = 'popup'
        $("#mce-EMAIL").val($("#popup-email").val())
        $("#mce-NAME").val($("#popup-name").val())
        $("#mce-MSG").val($("#popup-msg").val())
        $("#mc-embedded-subscribe").click()
        return false
      }
      function sendNewsForm(){
        formSent = 'news'
        $("#mce-EMAIL").val($("#news-email").val())
        $("#mce-NAME").val($("#news-name").val())
        $("#mc-embedded-subscribe").click()
        return false
      }
    </script>
